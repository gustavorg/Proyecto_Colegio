                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Docente_Cursos" />
        </activity>
        <activity
            android:name=".Docente_Horario"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Lista_Alumnos"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
