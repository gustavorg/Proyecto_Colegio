<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="principal.android.utp.proyecto">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Alumno"></activity>
        <activity android:name=".Alumno_Cursos"></activity>
        <activity android:name=".Alumno_Horario"></activity>
        <activity android:name=".Alumno_Notas"></activity>
        <activity android:name=".Docente" android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity android:name=".Docente_Cursos" android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity android:name=".Docente_Horario" android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity android:name=".Lista_Alumnos" android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>
</manifest>